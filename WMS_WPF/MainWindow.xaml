<Window x:Class="WMS_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WMS_WPF"
        mc:Ignorable="d"
        Title="WMS" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Canvas>
        <Button Name="btn_cadastrarOp" Width="110" Height="50" Canvas.Left="30" Canvas.Top="10" Content="Cadastrar OP" Click="btn_cadastrarOp_Click"/>
        <Button Name="btn_cadastrarProduto" Width="110" Height="50" Canvas.Left="150" Canvas.Top="10" Content="Cadastrar Produto" Click="btn_cadastrarProduto_Click"/>
        <Button Width="50" Height="50" Canvas.Left="30" Canvas.Top="90"/>
        <Button Width="50" Height="50" Canvas.Left="90" Canvas.Top="90"/>
        <Button Width="50" Height="50" Canvas.Left="150" Canvas.Top="90"/>
    </Canvas>
</Window>
